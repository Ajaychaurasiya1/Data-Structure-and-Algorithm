
#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    int g;
    int ig;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&g);
        if(g<38)
        {
            printf("%d\n",g);
        }
        else
        {
          ig=(g/5+1)*5;
          if(ig-g<3)
          printf("%d\n",ig);
          else {
          printf("%d\n",g);
          }
        }
    }
    
    return 0;
}
